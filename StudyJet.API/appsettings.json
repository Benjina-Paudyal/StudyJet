    "Imagepaths": {
        "Courses": "wwwroot/images/courses",
        "Profiles": "wwwroot/images/profiles"
    },

    "FilePaths": {
        "CvPath": "wwwroot/uploads/cv"
    },

    "DefaultProfilePicPaths": {
