{


    // Laptop
    "ConnectionStrings": {
        "DefaultConnection": "Server=localhost;Database=StudyJetDb;User Id=sa;Password=${DB_PASSWORD};TrustServerCertificate=True;"
    },


    // Desktop
    //"ConnectionStrings": {
    //   "DefaultConnection": "Server=BHASBENZ;Database=StudyJetDb;User Id=sa;Password=${DB_PASSWORD};TrustServerCertificate=True;"
